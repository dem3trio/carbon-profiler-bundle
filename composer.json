{
    "name": "dem3trio/carbon-profiler-bundle",
    "description": "Symfony 2/3 profiler extension, to change the Carbon php date in the whole project.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel GonzÃ¡lez",
            "email": "dgzaballos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "nesbot/carbon": "^1.22",
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/http-foundation": "~2.8|~3.0",
        "symfony/web-profiler-bundle": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/form": "~2.8|~3.0",
        "symfony/routing": "~2.8|~3.0",
        "symfony/event-dispatcher": "~2.8|~3.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {"Dem3trio\\Bundle\\CarbonProfilerBundle\\" : "src/"}
